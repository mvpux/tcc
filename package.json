{
  "name": "my-project",
  "version": "1.0.0",
  "description": "Projeto React + Vite com integração de Design Tokens do Figma",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "build:tokens": "style-dictionary build --config style-dictionary.config.js",
    "clean:tokens": "rimraf build/",
    "fetch:tokens": "figma-export tokens --config tokens/figma-settings.js",
    "lint": "eslint . --ext .ts,.tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "test": "echo 'No tests configured yet'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mvpux/tcc/my-project.git"
  },
  "keywords": [
    "react",
    "vite",
    "typescript",
    "style-dictionary",
    "design-tokens"
  ],
  "author": "Marcus Sousa",
  "license": "ISC",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.24",
    "@types/react": "^18.2.56",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "style-dictionary": "^3.9.2",
    "typescript": "^5.3.3",
    "vite": "^5.1.4"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}